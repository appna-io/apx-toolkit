{
  "name": "@apx-ui/toolkit",
  "version": "1.0.6",
  "type": "module",
  "description": "Universal Web Utility SDK for apx-ui (AppNA UX/UI) frontend applications",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint:fix": "eslint -c ./.eslintrc --fix 'src/**/*.{ts,tsx}'",
    "lint": "eslint -c ./.eslintrc .",
    "clean": "rm -rf dist",
    "prepublishOnly": "npm run clean && npm run build",
    "upload": "npm version patch --no-git-tag-version && npm publish",
    "build:docs": "npm run build && node docs/src/build-browser.js",
    "serve:docs": "cd docs && python3 -m http.server 8080"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/appna-io/apx-toolkit.git"
  },
  "keywords": [
    "utility",
    "validation",
    "formatters",
    "date",
    "currency",
    "regex",
    "typescript",
    "frontend",
    "web"
  ],
  "author": "AppNA UX Team",
  "license": "ISC",
  "devDependencies": {
    "@types/jest": "^29.5.0",
    "@types/node": "^20.0.0",
    "eslint-plugin-apx-eslint": "^1.0.0",
    "jest": "^29.5.0",
    "ts-jest": "^29.1.0",
    "typescript": "^5.0.0"
  },
  "dependencies": {}
}
